	file = {:D$\backslash$:/Users/gciatto/OneDrive - Alma Mater Studiorum Universit{\`{a}} di Bologna/Papers/2017/Kim et al/Interpretability Beyond Feature Attribution Quantitative Testing with Concept Activation Vectors (TCAV)/Interpretability Beyond Feature Attribution Quantitat.pdf:pdf},
	mendeley-groups = {XAI},
