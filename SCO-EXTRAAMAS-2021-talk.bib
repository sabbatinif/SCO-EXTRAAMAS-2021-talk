	mendeley-groups = {Symbolic-VS-Subsymbolic,XAI},
	pages = {1--58},
	title = {{Neural-Symbolic Learning and Reasoning: A Survey and Interpretation}},
	url = {http://arxiv.org/abs/1711.03902},
	year = {2017}
}
