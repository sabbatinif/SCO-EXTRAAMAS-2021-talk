	file = {:D$\backslash$:/Users/gciatto/OneDrive - Alma Mater Studiorum Universit{\`{a}} di Bologna/Papers/1983/Zhou, Jiang, Chen/Extracting Symbolic Rules from Trained Neural Network Ensembles/Extracting Symbolic Rules from Trained Neural Network Ensembles, Zhou, Jiang, Chen, 1.pdf:pdf},
