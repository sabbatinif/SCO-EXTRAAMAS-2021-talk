	title = {{Extracting Symbolic Rules from Trained Neural Network Ensembles}},
	url = {https://content.iospress.com/download/ai-communications/aic272?id=ai-communications{\%}2Faic272{\%}0Ahttp://portal.acm.org/citation.cfm?id=1218644},
